loadstring(game:HttpGet("https://raw.githubusercontent.com/typical-overk1ll/scripts/main/Invite",true))()local a=game:GetService("Players").LocalPlayer;local b=loadstring(game:HttpGet("https://raw.githubusercontent.com/typical-overk1ll/scripts/main/UILibrary",true))()local c=workspace:WaitForChild("SessionVehicles")local d=b.new("Juan's GV script :)")local e=d:addPage("engine hacks")local f=d:addPage("turbo hacks")local g=200;local h=false;local i=0;local j=0;local k=false;local l=e:addSection("tuning")l:addSlider("engine power 💯",0,0,1200,function(m)g=m end)l:addToggle("racing transmission (better top speed",false,function(m)h=m end)l:addButton("goodluck 🤯🤯")local n=f:addSection("SUPER CHARGER 🤑🤑")local o=f:addSection("SUPER CHARGER 👀")local p=d:addPage("Modifications"):addSection("other hacks")o:addSlider("turbo charger 🐌",0,0,4,function(m)i=m end)n:addSlider("SUPER CHARGER 🤑🤑🤑",0,0,2,function(m)j=m end)p:addToggle("good brakes",false,function(m)k=m end)while wait()do pcall(function()local q=require(c:FindFirstChild(a.Name.."-Car"):FindFirstChild("A-Chassis Tune"))q.Horsepower=g;q.E_Horsepower=g;if h then q.FinalDrive=2.5;q.Ratios={3.7,0,2.84,1.55,1,0.65}end;q.ClutchTol=10000;q.Redline=10000;q.TurboEnabled=true;q.Turbochargers=i;q.Superchargers=j;q.Weight=3;if k then local q=require(c:FindFirstChild(a.Name.."-Car"):FindFirstChild("A-Chassis Tune"))q.BrakeForce=10000000000000;q.PBrakeForce=10000000000000 end end)end
